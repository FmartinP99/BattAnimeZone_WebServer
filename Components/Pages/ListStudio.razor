@using BattAnimeZone.Services
@using BattAnimeZone.Components.Models.ProductionEntity
@inject AnimeService animeService
@page "/studiolist"

<SearchBar></SearchBar>

@foreach(var prod in animeProducers)
{
    @prod.Value.Url<br />
}


@code {

    Dictionary<int, ProductionEntity> animeProducers;


    protected override async Task OnInitializedAsync()
    {
        animeProducers = await animeService.GetProductionEntities();
        base.OnInitialized();
    }

}
